{
  "extends": "tslint:recommended",
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": [
      false
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "array-type": [
      true,
      "generic"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ],
    "radix": false,
    "no-unused-expression": false
  }
}
